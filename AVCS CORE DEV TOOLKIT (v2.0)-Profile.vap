			checkbox.CheckedChanged += (s, e) =&gt; GetUpdatedSizeTotalKb(cbV1, cbV2, cbBms,// cbRon,
					sizeFilesCoreV1, sizeFilesCoreV2,
					sizeFilesBmsV1, sizeFilesBmsV2,
					//sizeFilesRonV1, sizeFilesRonV2,
