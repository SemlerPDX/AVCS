		return Tuple.Create(
			dr == DialogResult.OK &amp;&amp; cbV1.Checked,
			dr == DialogResult.OK &amp;&amp; cbV2.Checked,
			dr == DialogResult.OK &amp;&amp; cbBms.Checked,
			false //dr == DialogResult.OK &amp;&amp; cbRon.Checked
		);
